@using Beehouse.Essentials.Web.Views;

<div class="container container-fluid">
    <div class="row">
        <div class="col-12">
            <h2>@Title</h2>
        </div>
    </div>

    @ChildContent
</div>

@functions
{
    [Parameter]
    private string Title { get; set; }

    [Parameter]
    private RenderFragment ChildContent { get; set; }
}